                    "format": "password",
                    "pattern": "[A-Za-z0-9-_=.]+",
                    "type": "string"
                },
                "AttributeName": {
                    "description": "The attribute name returned by the server response to get the user attribute verification code.",
                    "maxLength": 32,
                    "minLength": 1,
                    "pattern": "[\\p{L}\\p{M}\\p{S}\\p{N}\\p{P}]+",
                    "type": "string"
                }
            },
            "required": [
                "AccessToken",
                "AttributeName"
            ],
            "type": "object"
        },
